; label-connected = %local_ip% %downspeed% %upspeed%
label-connected = %local_ip%

format-disconnected = <label-disconnected>
format-disconnected-prefix = 賓
format-disconnected-prefix-foreground = ${colors.inactive}
label-disconnected = " "

